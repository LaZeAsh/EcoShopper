{
    "name": "Some Name",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "sustainable products on amazon",
    "permissions": ["tabs", "scripting", "background", "webNavigation", "storage"],
    "host_permissions": ["https://*.amazon.com/*"],
    "author": "La Hacks",
    "action":{
        "default_popup": "index.html",
        "default_title": "Some Name"
    },
    "background": {
        "service_worker": "background.js"
    }
}